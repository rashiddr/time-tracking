<%= form_for :status_report do |f| %>
  <p>
    <%= f.label :Date %><br>
    <%= f.date_field(:status_date, autofocus: true) %>
  </p>
  <p>
    <%= f.label :Project%>
    <%= project_list.first %><br>
  </p
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
